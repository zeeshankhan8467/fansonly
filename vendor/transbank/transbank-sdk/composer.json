  "keywords": ["payments", "transbank", "api", "sdk"],
  "require": {
      "php": ">=5.6.0",
      "ext-curl": "*",
      "ext-json": "*",
      "ext-soap": "*",
      "ext-mbstring": "*",
      "guzzlehttp/guzzle":"^5|^6|^7",
      "ext-openssl": "*"
  },
    "require-dev": {
      "phpunit/phpunit": "~4.0",
      "satooshi/php-coveralls": "~0.6.1",
      "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
      "psr-4": {
        "Transbank\\": [
          "src/"
